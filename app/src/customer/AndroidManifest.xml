<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application>

        <meta-data
            android:name="com.here.sdk.access_key_id"
            android:value="@string/ACCESS_KEY_ID" />
        <meta-data
            android:name="com.here.sdk.access_key_secret"
            android:value="@string/ACCESS_KEY_SECRET" />

        <activity
            android:name="com.example.shoppingapp_customer.navigation.NavigationActivity"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name="com.example.shoppingapp_customer.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.shoppingapp_customer.register.RegisterActivity"
            android:windowSoftInputMode="adjustPan" />
    </application>

</manifest>